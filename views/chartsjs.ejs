<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="refresh" content="500">
    <title>Graficas</title>

    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/chartjs-plugin-annotation/0.5.7/chartjs-plugin-annotation.min.js">
    </script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">


    <link rel="stylesheet" type="text/css" href="/stylesheets/graphs.css">
</head>

<body>
    <%- include(navbar) %>
        <script src=" /js/GraficasJs/parameters.js"></script>

        <div class="main-container">
            <div class="second-container">
                <%- include(chart_legend, {title: "Coil Resistance",id: "CR"}) %>
                    <%- include(chart_legend, {title: "Operate Voltage",id: "OV"}) %>
                        <%- include(chart_legend, {title: "Release Voltage", id: "RV"}) %>
                            <%- include(chart_legend, {title: "Static CR", id: "SCR"}) %>
                                <%- include(chart_legend, {title: "Operate Time", id: "OT"}) %>
                                    <%- include(chart_legend, {title: "Release Time", id: "RT"}) %>

                                        <script src=" /js/GraficasJs/graph.js"></script>
            </div>
        </div>

        <script src="/js/GraficasJs/letsDoItResponsive.js"></script>

        <div class="buttons dropdown col-5">
            <select id="select" onchange="changeVisibility()">
                <option value="CR">Coil Resistance</option>
                <option value="OV">Operate Voltage</option>
                <option value="RV">Release Voltage</option>
                <option value="OT">Operate Time</option>
                <option value="RT">Release Time</option>
                <option value="SCR">Static Contact Resistance</option>
            </select>
        </div>

        <%- include(footer) %>
</body>

</html>